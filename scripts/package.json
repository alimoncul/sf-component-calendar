{
  "name": "@smartface/sf-component-calendar",
  "version": "2.1.1",
  "description": "Calendar component",
  "main": "components/calendar.js",
  "scripts": {
    "prepublish": "npm run copytheme && cp -r ./node_modules/library ./library && rm -f ./library/styler-builder.js",
    "copytheme": "cp ../themes/workspaceTheme/styles/common.json ./theme.json",
    "postpublish": "rm -r -f ./library",
    "postinstall": "if [ ! -d node_modules ]; then mkdir node_modules;fi && mv -f ./library ./node_modules/library",
    "installSelf": "config=$(node -p \"JSON.stringify(require('./package.json').sfModule)\") && smm --config=$config || echo \"smm cannot be found\"",
    "docs": "rm -rf ../docs/* && node ../jsdocs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartface/sf-component-calendar.git"
  },
  "keywords": [
    "javascript",
    "mobile",
    "styling",
    "style"
  ],
  "sfModule": {
    "type": "Component",
    "moduleAssets": {
      "images": [],
      "fonts": [],
      "styles": []
    },
    "integration": {
      "type": "Calendar",
      "icon": "if it is a image path then it must be imported to UiEditor or a existing component type can be used like FlexLayout, ListView etc."
    }
  },
  "homepage": "https://github.com/smartface/sf-component-calendar#readme",
  "author": "Cenk Cetinkaya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartface/sf-component-calendar/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@smartface/html-to-text": "^1.0.6",
    "moment": "2.22.2",
    "moment-hijri": "2.1.1"
  },
  "devDependencies": {
    "@smartface/contx": "^1.2.0",
    "@smartface/styler": "^1.3.15",
    "js-base": "^1.0.0",
    "sf-core": "*",
    "sf-extension-utils": "^4.0.0"
  }
}
